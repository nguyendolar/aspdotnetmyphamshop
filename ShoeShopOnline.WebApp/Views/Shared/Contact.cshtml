@model IEnumerable<ShoeShopOnline.Model.DAL.Contact>
@{
    int i = 0;
}

<div class="footer" id="Contact">
    <div class="container">
        <div class="footer-top">
            @if (ViewBag.Count > 0)
            {
                foreach (var item in Model)
                {
                    <div class="col-md-4 footer-left">
                        <h3>@item.Title</h3>
                        <ul>
                            <li><b>@item.ObjectName</b></li>
                            <li>Địa chỉ: <a href="#">@item.Address</a></li>
                            <li>Sđt: <a href="#">@item.Mobile</a></li>
                            <li>Website: <a href="#">@item.Website</a></li>
                            <li>Email: <a href="#">@item.Email</a></li>
                        </ul>
                    </div>
                }
                <div class="col-md-4 footer-left">
                    <h3>Bản Đồ</h3>
                    <ul>
                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3724.185241246236!2d105.84769501533216!3d21.025272793272354!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3135ab9480ded35b%3A0xaab01c0c9e52a5d8!2zNDggSGFpIELDoCBUcsawbmcsIFRyw6BuZyBUaeG7gW4sIEhvw6BuIEtp4bq_bSwgSMOgIE7hu5lpLCBWaeG7h3QgTmFt!5e0!3m2!1svi!2s!4v1608382617323!5m2!1svi!2s" width="300" height="200" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
                    </ul>
                </div>
            }
            <div class="clearfix"> </div>
        </div>
    </div>
</div>